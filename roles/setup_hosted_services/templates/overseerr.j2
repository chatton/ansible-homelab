---
version: "3"
services:
  overseerr:
    labels:
      ie.cianhatton.backup.enabled: "true"
      ie.cianhatton.backup.key: "{{backups.schedule_keys.nightly}}"
    image: sctx/overseerr:latest
    container_name: overseerr
    ports:
      - 5055:5055
    environment:
      - PUID=1000
      - PGID=1000
      - TZ=Europe/London
    volumes:
      - "config:/app/config"
    restart: unless-stopped
volumes:
  config:
